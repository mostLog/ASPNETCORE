
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title></title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <link href="~/lib/layui/css/layui.css" rel="stylesheet" />
    <link href="~/css/common/layuie.css" rel="stylesheet" />
    <link href="~/lib/animate.css/animate.min.css" rel="stylesheet" />
    <link href="~/css/common/font/iconfont.css" rel="stylesheet" />
    <link href="~/css/buiness/Proxy/index.css" rel="stylesheet" />
    <base target="_blank">
</head>
<body style="padding:15px;">
    <div class="wrapper wrapper-content animated fadeInUp">
        <div class="panel_box row">
            <div class="panel col">
                <a href="javascript:;" >
                    <div class="panel_icon">
                        <i class="iconfont icon-ip" ></i>
                    </div>
                    <div class="panel_word">
                        <span id="ava-ip">5</span>
                        <cite>可用ip</cite>
                    </div>
                </a>
            </div>
            <div class="panel col">
                <a href="javascript:;" >
                    <div class="panel_icon" style="background-color:#FF5722;">
                        <i class="iconfont icon-ip1" ></i>
                    </div>
                    <div class="panel_word">
                        <span id="val-ip">3</span>
                        <cite>正在验证ip</cite>
                    </div>
                </a>
            </div>
        </div>
        <blockquote class="layui-elem-quote">
            <div class="layui-inline">
                <div class="layui-input-inline">
                    <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="请输入位置" class="layui-input">
                </div>
                <button type="submit" class="layui-btn">搜索</button>
            </div>
            <div class="layui-inline">
                <button id="btn-add" class="layui-btn">新建代理</button>
            </div>
        </blockquote>
        <div class="data-list">
            <table id="proxy-table" lay-filter="proxy-table"></table>
        </div>
    </div>
    <script id="proxy-type" type="text/html">
        {{# if(d.type==0){ }}
        <span>HTTP</span>
        {{# }else{ }}
        <span>HTTPS</span>
        {{# } }}
    </script>
    <script type="text/html" id="proxy-bar">
        <a class="layui-btn layui-btn-small" lay-event="edit">编辑</a>
        <a class="layui-btn layui-btn-danger layui-btn-small" lay-event="del">删除</a>
    </script>
    <script src="~/lib/layui/layui.js"></script>
    <script src="~/js/business/proxy/Index.js"></script>
</body>
</html>
