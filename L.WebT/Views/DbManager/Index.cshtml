@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title></title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <link href="~/lib/layui/css/layui.css" rel="stylesheet" />
    <link href="~/css/common/layuie.css" rel="stylesheet" />
    <link href="~/lib/animate.css/animate.min.css" rel="stylesheet" />
    <link href="~/css/common/font/iconfont.css" rel="stylesheet" />
    <base target="_blank">
</head>
<body style="padding:15px;">
    <div class="wrapper wrapper-content animated fadeInUp">
        <blockquote class="layui-elem-quote">
            <div class="layui-inline">
                <div class="layui-input-inline">
                    <input type="text" class="layui-input" name="search-record-date" id="search-record-date"  placeholder="请选择日期">
                </div>
                <button type="submit" class="layui-btn">搜索</button>
            </div>
            <div class="layui-inline">
                <input type="button" id="btn-Data" class="layui-btn" value="数据备份" />
            </div>
        </blockquote>
        <div class="data-list">
            <table id="db-table" lay-filter="db-table"></table>
        </div>
    </div>
    <script id="db-rows" type="text/html">
        {{# if(d.rows>=10000){ }}
        <span>{{(d.rows/10000).toFixed(2)}}万条</span>
        {{# }else{ }}
        <span>{{d.rows}}条</span>
        {{# } }}
    </script>
    <script id="db-reserved" type="text/html">
        {{#  if(d.reserved > 1024*1024){ }}
        <div>{{(d.reserved/(1024*1024)).toFixed(2)}}GB</div>
        {{#  } else if(d.reserved > 1024){ }}
        <div>{{(d.reserved/1024).toFixed(2)}}MB</div>
        {{#  } else{ }}
        <div>{{d.reserved}}KB</div>
        {{# } }}
    </script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/layui/layui.js"></script>
    <script src="~/js/business/dbmanager/Index.js"></script>
</body>
</html>