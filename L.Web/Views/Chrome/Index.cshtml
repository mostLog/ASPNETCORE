
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/lib/layui/css/layui.css" rel="stylesheet" />
    <link href="~/css/common/layuie.css" rel="stylesheet" />
    <link href="~/lib/animate.css/animate.min.css" rel="stylesheet" />
    <link href="~/css/common/font/iconfont.css" rel="stylesheet" />
    <base target="_blank">
</head>
<body style="padding:15px;">
    <div class="wrapper wrapper-content animated fadeInUp">
        <blockquote class="layui-elem-quote">
           
        </blockquote>
        <div class="data-list">
            <table id="pushtext-table" lay-filter="pushtext-table"></table>
        </div>
    </div>
    <script type="text/html" id="pushtext-bar">
        <a class="layui-btn layui-btn-warm layui-btn-sm" lay-event="check">审核</a>
        <a class="layui-btn layui-btn-sm" lay-event="edit">编辑</a>
        <a class="layui-btn layui-btn-danger layui-btn-sm" lay-event="del">删除</a>
    </script>
    <script src="~/lib/layui/layui.js"></script>
    <script src="~/js/business/chrome/Index.js"></script>
</body>
</html>
