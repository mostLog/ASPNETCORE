                                    <div class="layui-inline">
                                        <input  type="button" id="btn-Data" class="layui-btn" value="数据备份" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="data-list">
                            <table id="db-table" lay-filter="db-table"></table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/layui/layui.js"></script>
    <script src="~/js/business/dbmanager/Index.js"></script>
